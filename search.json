---
[
  {% for chapter in site.mobile %}
    {% unless chapter.hidden == true %}
    {
      "title"    : "{{ chapter.title | escape }}",
      "url"      : "{{ chapter.url | relative_url }}",
      "description" : "{{chapter.content | strip_html | strip_newlines | escape }}"
    } {% unless forloop.last %},{% endunless %}
    {% endunless %}
  {% endfor %}
]